@{
    ViewData["Title"] = "Contact";
}
<h2>@ViewData["Title"]</h2>
<h3>@ViewData["Message"]</h3>

<address>
    One Microsoft Way<br />
    Redmond, WA 98052-6399<br />
    <abbr title="Phone">P:</abbr>
    425.555.0100
</address>

<address>
    <strong>Support:</strong> <a href="mailto:Support@example.com">Support@example.com</a><br />
    <strong>Marketing:</strong> <a href="mailto:Marketing@example.com">Marketing@example.com</a>
</address>
<form role="form" class="form-inline">
    <div class="form-group">
        <label>
            出生年月
        </label>
        <input type="date" class="form-control" placeholder="user" />
    </div>

   
    <div class="form-group">
        <label class="sr-only">
            邮件
        </label>
        <input type="email" class="form-control" placeholder="enter email" />
    </div>
    <div class="form-group">
        <label  >
            密码
        </label>
        <input type="password" class="form-control" placeholder="输入密码" />
    </div>
    <div class="form-group">
        <label class="sr-only">
            选择文件
        </label>
        <input type="file"  />
        <p class="help-block">选择你需要的文件</p>
    </div>
</form>

<form class="form-horizontal" role="form">
    <div class="form-group">
        <label class="col-sm-1 control-label">Email</label>
        <input type="email" class="col-sm-5"
               placeholder="Email">
    </div>
    <div class="form-group">
        <label class="col-sm-1 control-label">密码</label>
        <input type="password" class="col-sm-5"
               placeholder="密码">
    </div>
    <div class="form-group">
        <label class="control-label col-sm-offset-1"><input type="checkbox" /> 记住密码</label>
    </div>
    <input type="text" class="form-control" />
    <textarea class="form-control" rows="5"></textarea>
    <div class="row">
        <div class="radio col-sm-1">
            <label>
                <input name="sexrd" type="radio" checked /> 男
            </label>
        </div>

        <div class="radio col-sm-1">
            <label>
                <input name="sexrd" type="radio" />  女
            </label>
        </div>
    </div>
    <select   class="form-control" >
        <option>1</option>
        <option>2</option>
        <option>3</option>
        <option>4</option>
        
    </select>
    <input type="text" class="form-control" disabled/>
    <p class="form-control-static"> 请按照我的格式输入</p>
</form>
<form role="form" class="form-horizontal"> 
    <div class="form-group has-warning ">
        <label class="col-sm-1 text-center control-label"> 警告</label>
       <div class="col-sm-5">
        <input class="form-control col-xs-5" type="text" />
    </div>
    </div>
    <div class="form-group has-error ">
        <label class="col-sm-1 text-center control-label"> 错误</label>
    <div class="col-sm-5">
        <input class="form-control" type="text" />
    </div>
    </div>
    <div class="form-group has-success ">
        <label class=" col-sm-1 text-center control-label"  > 成功</label>
        <div class="col-sm-5">
            <input class="form-control" type="text" />
        </div>
    </div>
    
</form>